<%= simple_form_for(@group) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :group_category %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :logo %>
    <%= f.input :banner %>
    <%= f.input :owner_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
